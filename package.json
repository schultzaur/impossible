{
  "name": "impossible",
  "version": "0.0.0",
  "description": "Various tools and game AIs for Clubhouse Games.",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "node tests",
    "asbuild:untouched": "asc assembly/index.ts -b build/untouched.wasm -t build/untouched.wat --sourceMap --debug",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --optimize",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "wpbuild": "webpack",
    "build": "npm run asbuild && npm run wpbuild",
    "serve": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schultzaur/impossible.git"
  },
  "author": "schultzaur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schultzaur/impossible/issues"
  },
  "homepage": "https://github.com/schultzaur/impossible#readme",
  "dependencies": {
    "@assemblyscript/loader": "^0.10.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "assemblyscript": "^0.12.3",
    "css-loader": "^3.6.0",
    "style-loader": "^1.2.1",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
